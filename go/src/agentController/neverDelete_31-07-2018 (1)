=============================== Sudo access right for a user ==================================

func hasRootAccess(usrName string) bool{
    cmd := "id -nG "+usrName
    status := agentUtil.ExecComand(cmd, "misc. L70")
    return (strings.Contains(status, "sudo"))
}

